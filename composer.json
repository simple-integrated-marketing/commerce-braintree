{
	"name": "kuriousagency/commerce-braintree",
	"description": "Braintree gateway for Commerce 2",
	"type": "craft-plugin",
	"version": "2.0.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"commerce braintree"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/commerce": "^2.0.0-alpha.6",
		"braintree/braintree_php": "^4.1.0"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\commerce\\braintree\\": "src/"
		}
	},
	"extra": {
		"name": "Braintree for Craft Commerce",
		"handle": "commerce-braintree",
		"hasCpSettings": false,
		"hasCpSection": false,
		"components": {
			"braintreeService": "kuriousagency\\commerce\\braintree\\services\\BraintreeService"
		},
		"class": "kuriousagency\\commerce\\braintree\\Braintree"
	}
}
